
@using RwaLib.MODELS
@{
    ViewBag.Title = "Dashboard";
    User user = (User)Session["user"];
    string msg = ViewBag.msg as string;
}
@if (msg != null)
{
    <div class="alert alert-info my-2" role="alert">
        @msg
    </div>
}
@if (user.IsAdmin)
{
    @Html.Partial("_AdminPage", user);
}
else
{
    @Html.Partial("_UserPage", user);
}